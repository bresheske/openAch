{
  "name": "openach",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run watch\" \"nodemon --exec \\\"npm run test\\\" \" ",
    "watch": "tsc -w",
    "tsc": "tsc",
    "scan": "sonar-scanner",
    "test": "nyc mocha spec/unitTests",
    "build": "nexe main.js -o dist/ach.exe",
    "e2e": "mocha spec/e2eTests",
    "deploy": "npm run tsc && npm run test && npm run build && npm run e2e"
  },
  "author": "bresheske",
  "license": "ISC",
  "nyc": {
    "exclude": [
      "spec/**"
    ]
  },
  "dependencies": {
    "async-file": "^2.0.2",
    "bunyan": "^1.8.12",
    "minimist": "^1.2.6",
    "uuid": "^3.3.2",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^8.10.21",
    "@types/uuid": "^3.4.3",
    "chai": "^4.1.2",
    "concurrently": "^3.6.1",
    "mocha": "^5.2.0",
    "nexe": "^2.0.0-rc.31",
    "nodemon": "^1.18.3",
    "nyc": "^12.0.2",
    "typescript": "^2.9.2"
  }
}
